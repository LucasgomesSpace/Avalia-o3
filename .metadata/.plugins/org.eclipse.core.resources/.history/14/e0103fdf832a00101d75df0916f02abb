package atividades;

import java.util.Scanner;

public class VariosNumerosInteiros {

	public static void main(String[] args) {
		
		Scanner ler = new Scanner(System.in);
		int numD;
		int soma;
		
		do {
		System.out.println("Digite numeros positivos para somar");
		numD = ler.nextInt();
		soma =+ numD;
		} while (numD<0);
		
		System.out.println("SOMA IGUAL A " + soma);
	}

}
