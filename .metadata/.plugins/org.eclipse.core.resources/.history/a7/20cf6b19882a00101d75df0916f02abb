package atividades;

import java.util.Scanner;

public class Tabuada {

	public static void main(String[] args) {
		
		Scanner ler = new Scanner(System.in);
		System.out.println("DIGITE UM NUMERO INTEIRO PARA SABER A TABUADA :");
		int num = ler.nextInt();
		int multi;
		
		
		for(int i =0; i<=10; i++) {
			multi = num * i; 
			System.out.println(i +" X " + num + " = " + multi);
		}
		ler.close();
	}

}
